var usuario = prompt("insira seu usuário", 0)
var senha = prompt("insira sua senha", 0)

while (usuario == senha){
  alert("ERRO")
  var usuario = prompt("insira seu usuário, novamente", 0)
  var senha = prompt("insira sua senha, novamente", 0)
}

if (usuario != senha){
  alert("Conta criada")
  console.log("Conta criada!")
}
